<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appcarichi">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CAMERA" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="CARICHI PREVISTI"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ListViewEx"
        android:networkSecurityConfig="@xml/network_security_config">
        <activity android:name="com.appcarichi.activities.SpuntaColloActivity"></activity>
        <activity android:name="com.appcarichi.activities.CheckNoteActivity"
            android:theme="@android:style/Theme.Material.Light.DialogWhenLarge.NoActionBar"></activity>
        <activity android:name="com.appcarichi.activities.InsertNotaActivity"
            android:theme="@android:style/Theme.Material.Light.DialogWhenLarge.NoActionBar"></activity>
        <activity android:name="com.appcarichi.activities.MainActivity"></activity>
        <activity android:name="com.appcarichi.activities.RigheOrdineActivity"></activity>
        <activity android:name="com.appcarichi.activities.OrdineActivity"></activity>
        <activity android:name="com.appcarichi.activities.LoginActivity"
            android:exported="true"
            android:configChanges="orientation|screenSize|keyboard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application></manifest>